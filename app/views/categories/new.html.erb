

<h1>New Category</h1>

<%= form_for(@category) do |f| %>

  <div class="name">
<%= f.label :name %> <br>
<%=f.text_field :name %>
  </div>
  <div class="thumburl">
<%= f.label :thumburl %><br>
   <%= f.url_field :thumburl %>
  </div>
  <div class="actions">
 <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Back', categories_path %>

